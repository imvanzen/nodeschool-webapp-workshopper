{
  "name": "nodeschool-webapp-workshopper",
  "description": "NodeSchool Silesia Workshopper for January 2016",
  "scripts": {
    "build": "npm-run-all -p build-server build-client",
    "build-server": "babel es6 -d es5",
    "build-client": "NODE_ENV=production webpack -p --config webpack/webpack.config.js && bower install",
    "dev": "npm-run-all -p dev-client dev-server",
    "dev-client": "babel-node webpack && bower install",
    "dev-server": "nodemon -w es6 -x babel-node es6",
    "start": "NODE_ENV=production node es5",
    "clean": "rimraf es5 /s /q && rimraf public/bower_components /s /q",
    "postinstall": "npm-run-all -s clean build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imvanzen/nodeschool-webapp-workshopper.git"
  },
  "author": "@imvanzen",
  "contributors": [
    {
      "name": "@p-janik"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imvanzen/nodeschool-webapp-workshopper/issues"
  },
  "engines": {
    "node": "~5.1.0"
  },
  "homepage": "https://github.com/imvanzen/nodeschool-webapp-workshopper#readme",
  "dependencies": {
    "babel": "~5.8.34",
    "bluebird": "~3.1.1",
    "body-parser": "~1.14.2",
    "bower": "^1.7.2",
    "express": "~4.13.3",
    "lodash": "^4.0.0",
    "morgan": "~1.6.1",
    "react": "^0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.6",
    "swig": "~1.4.2",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "axios": "^0.9.0",
    "babel-eslint": "~4.1.6",
    "babel-loader": "^5.4.0",
    "eslint": "~1.10.3",
    "node-sass": "^3.4.2",
    "nodemon": "~1.8.1",
    "npm-run-all": "~1.4.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
